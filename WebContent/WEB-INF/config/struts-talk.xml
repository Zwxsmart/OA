<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

    <constant name="struts.action.extension" value="action,,do" />
    <constant name="struts.multipart.maxSize" value="20971520" />

    <package name="talk-struts" extends="my-struts" namespace="/talk">
        <action name="*" class="com.cs.action.TalkAction" method="{1}">
        	<result name="skip">/WEB-INF/views/student/talk.jsp</result>
        	<result name="skips">/WEB-INF/views/student/talk.jsp</result>
        	<result name="admissionsSkip">/WEB-INF/admissions/student/talk.jsp</result><!-- 招生部主任 -->
        	<result name="NoPrevillige">/WEB-INF/views/NoPrevillige.jsp</result>
            <result name="save" type="json"/>
            <result name="all" type="json"/>
            <result name="alls" type="json"/>
            <result name="update" type="json"/>
            <result name="updateFreeze" type="json"/>
             <result name="freezeAll" type="json"/>
            <result name="blurredAllQuery" type="json"/>
            <result name="input" type="json"></result>
        </action>
    </package>
</struts>