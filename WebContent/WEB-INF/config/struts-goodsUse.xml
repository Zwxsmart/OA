<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

    <constant name="struts.action.extension" value="action,,do" />
    <constant name="struts.multipart.maxSize" value="20971520" />

    <package name="goodsUse-struts" extends="my-struts" namespace="/goodsUse">
        <action name="*" class="com.cs.action.GoodsUseAction" method="{1}">
        	<result name="skip">/WEB-INF/views/emp/goodsUse.jsp</result>
        	<result name="admissionsSkip">/WEB-INF/admissions/emp/goodsUse.jsp</result><!-- 招生部主任 -->
            <result name="save" type="json"/>
            <result name="all" type="json"/>
            <result name="update" type="json"/>
            <result name="delete" type="json"/>
            <result name="goodsApplyPage">/WEB-INF/emp/message/goodsUse.jsp</result>
            <result name="goodsApply" type="json"/>
            <result name="updateUseStatus" type="json"/>
            <result name="goodsUseAct" type="json"/>
            <result name="goodsUseDis" type="json"/>
            <result name="okUse" type="json"/>
            <result name="blurredAllQuery" type="json"></result><!-- 模糊查询 -->
            <result name="serachtime" type="json"/><!-- 时间段查询 -->
            <result name="NoPrevillige">/WEB-INF/views/NoPrevillige.jsp</result> <!-- 错误页面 -->
        </action>
    </package>
</struts>