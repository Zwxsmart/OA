<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

    <constant name="struts.action.extension" value="action,,do" />
    <constant name="struts.multipart.maxSize" value="20971520" />
    
	<package name="salaryInfo-struts" extends="my-struts" namespace="/salaryInfo">
        <action name="*" class="com.cs.action.SalaryInfoAction" method="{1}">
        	<result name="salaryInfo">/WEB-INF/views/salary/salaryInfo.jsp</result>
        	<result name="admissionsSkip">/WEB-INF/admissions/salary/salaryInfo.jsp</result><!-- 招生部主任 -->
            <result name="save" type="json"/><!-- 添加工資基本信息 -->
            <result name="all" type="json"/><!-- 分页查询全部工资信息-->
            <result name="update" type="json"/><!-- 修改工资信息 -->
          <result name="id" type="json"></result> <!-- 查询单工资信息详情    -->  
          <result name="NoPrevillige">/WEB-INF/views/NoPrevillige.jsp</result>
        </action>
    </package>
</struts>